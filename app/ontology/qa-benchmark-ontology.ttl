<http://purl.com/qado/ontology.ttl#hasWebqspQuestion> rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf <urn:qado:dataset#cwqProperty> ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                             rdfs:range xsd:string ;
                             rdfs:comment "Question text in web QSP format"@en ;
