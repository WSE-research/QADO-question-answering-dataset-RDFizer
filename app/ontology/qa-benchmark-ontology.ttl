#################################################################

[ rdf:type owl:Restriction ;
  owl:onProperty qado:hasAnswer ;
  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onClass qado:Answer ;
  rdfs:subClassOf qado:Question
] .


[ rdf:type owl:Restriction ;
  owl:onProperty qado:hasAnswer ;
  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onClass qado:AnswerContent ;
  rdfs:subClassOf qado:Answer
] .


[ rdf:type owl:Restriction ;
  owl:onProperty qado:hasSparqlQuery ;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onClass qado:Query ;
  rdfs:subClassOf qado:Question
] .


[ rdf:type owl:Restriction ;
  owl:onProperty qado:isElementOf ;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onClass qado:Benchmark ;
  rdfs:subClassOf qado:Question
] .


[ rdf:type owl:Restriction ;
  owl:onProperty qado:hasId ;
  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onDataRange xsd:nonNegativeInteger ;
  rdfs:subClassOf [ rdf:type owl:Class ;
                    owl:unionOf ( qado:Answer
                                  qado:Question
                                )
                  ]
] .


[ rdf:type owl:Restriction ;
  owl:onProperty qado:hasId ;
