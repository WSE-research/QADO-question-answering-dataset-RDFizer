<urn:qado:vocabulary> rdf:type owl:Ontology ;
                      owl:versionIRI <urn:qado:dataset/0.1> ;
                      rdfs:comment "Ontology for Datasets representing Knowledge Graphs on Question Answering benchmarks."@en ;
                      rdfs:label "QADO - Question Answering Dataset Ontology"@en .

