<http://purl.com/qado/ontology.ttl#hasTemplateIndex> rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf <urn:qado:dataset#lcquad2Property> ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                            rdfs:range xsd:nonNegativeInteger ;
