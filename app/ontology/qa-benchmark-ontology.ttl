@prefix : <http://purl.com/qado/ontology.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qado: <http://purl.com/qado/ontology.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.com/qado/ontology.ttl#> .

<http://purl.com/qado/ontology.ttl#> rdf:type owl:Ontology ;
                                      owl:versionIRI <urn:qado:dataset/0.3.0> ;
                                      rdfs:comment "Ontology for datassets representing Knowledge Graphs on Question Answering (KGQA) benchmarks."@en ;
                                      rdfs:label "QADO - Question Answering Dataset Ontology"@en .

#################################################################
#    Datatypes
#################################################################

###  http://purl.com/qado/ontology.ttl#Question
qado:Question rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.com/qado/ontology.ttl##basicQADOObjectProperty
<http://purl.com/qado/ontology.ttl##basicQADOObjectProperty> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf owl:topObjectProperty ;
                                                             rdfs:comment "QADO object property available for all benchmarks"@en ;
                                                             rdfs:label "basic QADO object property"@en .


###  http://purl.com/qado/ontology.ttl##cwqObjectProperty
<http://purl.com/qado/ontology.ttl##cwqObjectProperty> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://purl.com/qado/ontology.ttl##extendedQADOObjectProperty> ;
                                                       rdfs:comment "QADO object property only available for CWQ benchmark"@en ;
                                                       rdfs:label "CWQ object property"@en .


###  http://purl.com/qado/ontology.ttl##extendedQADOObjectProperty
<http://purl.com/qado/ontology.ttl##extendedQADOObjectProperty> rdf:type owl:ObjectProperty ;
                                                                rdfs:comment "QADO Object Property only available for a subset of benchmarks"@en ;
                                                                rdfs:label "extended QADO object property"@en .


###  http://purl.com/qado/ontology.ttl##hasAnswerContent
<http://purl.com/qado/ontology.ttl##hasAnswerContent> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://purl.com/qado/ontology.ttl##cwqObjectProperty> ;
                                                      rdfs:domain <http://purl.com/qado/ontology.ttl#ValidAnswer> ;
                                                      rdfs:range <http://purl.com/qado/ontology.ttl#AnswerContent> ;
                                                      rdfs:comment "Object Property linking an Answer Object to its corresponding content."@en ;
                                                      rdfs:label "has Answer Content"@en .


###  http://purl.com/qado/ontology.ttl#hasAnswer
<http://purl.com/qado/ontology.ttl#hasAnswer> rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://purl.com/qado/ontology.ttl##basicQADOObjectProperty> ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                     rdfs:range <http://purl.com/qado/ontology.ttl#Answer> ;
                     rdfs:comment "Property linking a question to the corresponding answer."@en ;
                     rdfs:label "has answer"@en .


###  http://purl.com/qado/ontology.ttl#hasSparqlQuery
<http://purl.com/qado/ontology.ttl#hasSparqlQuery> rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://purl.com/qado/ontology.ttl##basicQADOObjectProperty> ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                          rdfs:range <http://purl.com/qado/ontology.ttl#Query> ;
                          rdfs:comment "Property linking a question to the corresponding SPARQL query."@en ;
                          rdfs:label "has SPARQL query"@en .


###  http://purl.com/qado/ontology.ttl#isElementOf
<http://purl.com/qado/ontology.ttl#isElementOf> rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://purl.com/qado/ontology.ttl##basicQADOObjectProperty> ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                       rdfs:range <http://purl.com/qado/ontology.ttl#Dataset> ;
                       rdfs:comment "Property connecting a question to its QA dataset."@en ;
                       rdfs:label "is element of"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.com/qado/ontology.ttl##computedQADOProperty
<http://purl.com/qado/ontology.ttl##computedQADOProperty> rdf:type owl:DatatypeProperty ;
                                                          rdfs:subPropertyOf owl:topDataProperty ;
                                                          rdfs:comment "QADO property computed on given basic properties"@en ;
                                                          rdfs:label "computed QADO property"@en .


###  http://purl.com/qado/ontology.ttl#createdAt
<http://purl.com/qado/ontology.ttl#createdAt> rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf <urn:qado:dataset#cwqProperty> ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "Timestamp of question generation"@en ;
                     rdfs:label "has creation time"@en .


###  http://purl.com/qado/ontology.ttl#hasAggregation
<http://purl.com/qado/ontology.ttl#hasAggregation> rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf <urn:qado:dataset#qaldProperty> ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                          rdfs:range xsd:boolean ;
                          rdfs:comment "SPARQL query of the question uses one or more aggregation functions"@en ;
                          rdfs:label "is containing aggregations"@en .


###  http://purl.com/qado/ontology.ttl#hasAlias
<http://purl.com/qado/ontology.ttl#hasAlias> rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf <urn:qado:dataset#cwqProperty> ;
                    rdfs:domain <http://purl.com/qado/ontology.ttl#AnswerContent> ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Property linking an alias of the given answer."@en ;
                    rdfs:label "has answer alias"@en .


###  http://purl.com/qado/ontology.ttl#hasAnswerBody
<http://purl.com/qado/ontology.ttl#hasAnswerBody> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf <urn:qado:dataset#mintakaProperty> ,
                                            <urn:qado:dataset#qaldProperty> ;
                         rdfs:domain <http://purl.com/qado/ontology.ttl#Answer> ;
                         rdfs:range [ rdf:type rdfs:Datatype ;
                                      owl:unionOf ( xsd:anyURI
                                                    xsd:boolean
                                                    xsd:string
                                                  )
                                    ] ;
                         rdfs:comment "Answer value"@en ;
                         rdfs:label "has answer body"@en .


###  http://purl.com/qado/ontology.ttl#hasAnswerText
<http://purl.com/qado/ontology.ttl#hasAnswerText> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf <urn:qado:dataset#cwqProperty> ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain <http://purl.com/qado/ontology.ttl#AnswerContent> ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The answer text of the given answer."@en ;
                         rdfs:label "has question answer"@en .


###  http://purl.com/qado/ontology.ttl#hasAnswerType
<http://purl.com/qado/ontology.ttl#hasAnswerType> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf <urn:qado:dataset#qaldProperty> ,
                                            <urn:qado:dataset#rubqProperty> ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( <http://purl.com/qado/ontology.ttl#Answer>
                                                     <http://purl.com/qado/ontology.ttl#Question>
                                                   )
                                     ] ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Answer type of a given answer."@en ;
                         rdfs:label "has answer type"@en .


###  http://purl.com/qado/ontology.ttl#hasCompositionAnswer
<http://purl.com/qado/ontology.ttl#hasCompositionAnswer> rdf:type owl:DatatypeProperty ;
                                rdfs:subPropertyOf <urn:qado:dataset#cwqProperty> ;
                                rdf:type owl:FunctionalProperty ;
                                rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                                rdfs:range xsd:string ;
                                rdfs:comment "Answer text of a composed question"@en ;
                                rdfs:label "has composition answer"@en .


###  http://purl.com/qado/ontology.ttl#hasCompositionalityType
<http://purl.com/qado/ontology.ttl#hasCompositionalityType> rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf <urn:qado:dataset#cwqProperty> ;
                                   rdf:type owl:FunctionalProperty ;
                                   rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "Type of composition done in this question"@en ;
                                   rdfs:label "has compositionality type"@en .


###  http://purl.com/qado/ontology.ttl#hasEntityUriInQuestion
<http://purl.com/qado/ontology.ttl#hasEntityUriInQuestion> rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf <urn:qado:dataset#rubqProperty> ;
                                  rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                                  rdfs:range xsd:anyURI ;
                                  rdfs:comment "The given question contains an entity with the given URI"@en ;
                                  rdfs:label "has entity"@en .


###  http://purl.com/qado/ontology.ttl#hasId
<http://purl.com/qado/ontology.ttl#hasId> rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf <urn:qado:dataset#basicQADOProperty> ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( <http://purl.com/qado/ontology.ttl#AnswerContent>
                                             <http://purl.com/qado/ontology.ttl#Question>
                                           )
                             ] ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:unionOf ( xsd:nonNegativeInteger
                                            xsd:string
                                          )
                            ] ;
                 rdfs:comment "Property linking a question to its ID."@en ;
                 rdfs:label "has question ID"@en .


###  http://purl.com/qado/ontology.ttl#hasKeyword
<http://purl.com/qado/ontology.ttl#hasKeyword> rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf <urn:qado:dataset#qaldProperty> ;
                      rdfs:domain <http://purl.com/qado/ontology.ttl#Question> ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Property linking a Question with a keyword."@en ;
                      rdfs:label "has question keyword"@en .


