@prefix : <urn:qado:dataset#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qado: <http://purl.com/qado/ontology.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.com/qado/ontology.ttl> .

<http://purl.com/qado/ontology.ttl> rdf:type owl:Ontology ;
                                     owl:versionIRI <urn:qado:dataset/0.3.0> ;
                                     rdfs:comment "Ontology for datassets representing Knowledge Graphs on Question Answering (KGQA) benchmarks."@en ;
                                     rdfs:label "QADO - Question Answering Dataset Ontology"@en .

#################################################################
#    Datatypes
#################################################################

###  urn:qado:dataset#Question
:Question rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.com/qado/ontology.ttl#basicQADOObjectProperty
qado:basicQADOObjectProperty rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             rdfs:comment "QADO object property available for all benchmarks"@en ;
                             rdfs:label "basic QADO object property"@en .


###  http://purl.com/qado/ontology.ttl#cwqObjectProperty
qado:cwqObjectProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf qado:extendedQADOObjectProperty ;
                       rdfs:comment "QADO object property only available for CWQ benchmark"@en ;
                       rdfs:label "CWQ object property"@en .


###  http://purl.com/qado/ontology.ttl#extendedQADOObjectProperty
qado:extendedQADOObjectProperty rdf:type owl:ObjectProperty ;
                                rdfs:comment "QADO Object Property only available for a subset of benchmarks"@en ;
                                rdfs:label "extended QADO object property"@en .


###  http://purl.com/qado/ontology.ttl#hasAnswerContent
qado:hasAnswerContent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf qado:cwqObjectProperty ;
                      rdfs:domain <urn:qado#ValidAnswer> ;
                      rdfs:range <urn:qado#AnswerContent> ;
                      rdfs:comment "Object Property linking an Answer Object to its corresponding content."@en ;
                      rdfs:label "has Answer Content"@en .


###  urn:qado#hasAnswer
<urn:qado#hasAnswer> rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf qado:basicQADOObjectProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <urn:qado#Question> ;
                     rdfs:range <urn:qado#Answer> ;
                     rdfs:comment "Property linking a question to the corresponding answer."@en ;
                     rdfs:label "has answer"@en .


###  urn:qado#hasSparqlQuery
<urn:qado#hasSparqlQuery> rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf qado:basicQADOObjectProperty ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain <urn:qado#Question> ;
                          rdfs:range <urn:qado#Query> ;
                          rdfs:comment "Property linking a question to the corresponding SPARQL query."@en ;
                          rdfs:label "has SPARQL query"@en .


###  urn:qado#isElementOf
<urn:qado#isElementOf> rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf qado:basicQADOObjectProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain <urn:qado#Question> ;
                       rdfs:range <urn:qado#Dataset> ;
                       rdfs:comment "Property connecting a question to its QA dataset."@en ;
                       rdfs:label "is element of"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.com/qado/ontology.ttl#computedQADOProperty
qado:computedQADOProperty rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdfs:comment "QADO property computed on given basic properties"@en ;
                          rdfs:label "computed QADO property"@en .


###  urn:qado#createdAt
<urn:qado#createdAt> rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :cwqProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <urn:qado#Question> ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "Timestamp of question generation"@en ;
                     rdfs:label "has creation time"@en .


###  urn:qado#hasAggregation
<urn:qado#hasAggregation> rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf :qaldProperty ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain <urn:qado#Question> ;
                          rdfs:range xsd:boolean ;
                          rdfs:comment "SPARQL query of the question uses one or more aggregation functions"@en ;
                          rdfs:label "is containing aggregations"@en .


###  urn:qado#hasAlias
<urn:qado#hasAlias> rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :cwqProperty ;
                    rdfs:domain <urn:qado#AnswerContent> ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Property linking an alias of the given answer."@en ;
                    rdfs:label "has answer alias"@en .


###  urn:qado#hasAnswerBody
<urn:qado#hasAnswerBody> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :mintakaProperty ,
                                            :qaldProperty ;
                         rdfs:domain <urn:qado#Answer> ;
                         rdfs:range [ rdf:type rdfs:Datatype ;
                                      owl:unionOf ( xsd:anyURI
                                                    xsd:boolean
                                                    xsd:string
                                                  )
                                    ] ;
                         rdfs:comment "Answer value"@en ;
                         rdfs:label "has answer body"@en .


###  urn:qado#hasAnswerText
<urn:qado#hasAnswerText> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :cwqProperty ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain <urn:qado#AnswerContent> ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The answer text of the given answer."@en ;
                         rdfs:label "has question answer"@en .


###  urn:qado#hasAnswerType
<urn:qado#hasAnswerType> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :qaldProperty ,
                                            :rubqProperty ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( <urn:qado#Answer>
                                                     <urn:qado#Question>
                                                   )
                                     ] ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Answer type of a given answer."@en ;
                         rdfs:label "has answer type"@en .


###  urn:qado#hasCompositionAnswer
<urn:qado#hasCompositionAnswer> rdf:type owl:DatatypeProperty ;
                                rdfs:subPropertyOf :cwqProperty ;
                                rdf:type owl:FunctionalProperty ;
                                rdfs:domain <urn:qado#Question> ;
                                rdfs:range xsd:string ;
                                rdfs:comment "Answer text of a composed question"@en ;
                                rdfs:label "has composition answer"@en .


###  urn:qado#hasCompositionalityType
<urn:qado#hasCompositionalityType> rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf :cwqProperty ;
                                   rdf:type owl:FunctionalProperty ;
                                   rdfs:domain <urn:qado#Question> ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "Type of composition done in this question"@en ;
                                   rdfs:label "has compositionality type"@en .


###  urn:qado#hasEntityUriInQuestion
<urn:qado#hasEntityUriInQuestion> rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf :rubqProperty ;
                                  rdfs:domain <urn:qado#Question> ;
                                  rdfs:range xsd:anyURI ;
                                  rdfs:comment "The given question contains an entity with the given URI"@en ;
                                  rdfs:label "has entity"@en .


###  urn:qado#hasId
<urn:qado#hasId> rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :basicQADOProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( <urn:qado#AnswerContent>
                                             <urn:qado#Question>
                                           )
                             ] ;
                 rdfs:range [ rdf:type rdfs:Datatype ;
                              owl:unionOf ( xsd:nonNegativeInteger
                                            xsd:string
                                          )
                            ] ;
                 rdfs:comment "Property linking a question to its ID."@en ;
                 rdfs:label "has question ID"@en .


###  urn:qado#hasKeyword
<urn:qado#hasKeyword> rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf :qaldProperty ;
                      rdfs:domain <urn:qado#Question> ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Property linking a Question with a keyword."@en ;
                      rdfs:label "has question keyword"@en .


###  urn:qado#hasMachineReadableQuestion
<urn:qado#hasMachineReadableQuestion> rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf :cwqProperty ;
                                      rdf:type owl:FunctionalProperty ;
                                      rdfs:domain <urn:qado#Question> ;
                                      rdfs:range xsd:string ;
                                      rdfs:comment "Machine readable form of the question text"@en ;
                                      rdfs:label "has machine-readable question"@en .


###  urn:qado#hasNNQTQuestion
<urn:qado#hasNNQTQuestion> rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf :lcquad2Property ;
                           rdf:type owl:FunctionalProperty ;
                           rdfs:domain <urn:qado#Question> ;
                           rdfs:range xsd:string ;
                           rdfs:comment "Question text in NNQT format"@en ;
                           rdfs:label "has NNQT question format"@en .


###  urn:qado#hasParaphrasedQuestion
<urn:qado#hasParaphrasedQuestion> rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf :lcquad2Property ;
                                  rdf:type owl:FunctionalProperty ;
                                  rdfs:domain <urn:qado#Question> ;
                                  rdfs:range xsd:string ;
                                  rdfs:comment "has paraphrased question"@en ,
                                               "has paraphrased version of question text."@en .


###  urn:qado#hasQueryText
<urn:qado#hasQueryText> rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :basicQADOProperty ;
                        rdfs:domain <urn:qado#Question> ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Property linking a question with the corresponding SPARQL query."@en ;
                        rdfs:label "has SPARQL query"@en .


###  urn:qado#hasQuestion
<urn:qado#hasQuestion> rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :basicQADOProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain <urn:qado#Question> ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Property linking a question with the question text."@en ;
                       rdfs:label "has question text"@en .


###  urn:qado#hasQuestionProperty
<urn:qado#hasQuestionProperty> rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf :rubqProperty ;
                               rdfs:domain <urn:qado#Question> ;
                               rdfs:range xsd:string ;
                               rdfs:comment "Property a question relates to."@en ;
                               rdfs:label "has question property"@en .


###  urn:qado#hasSPARQLResult
<urn:qado#hasSPARQLResult> rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf :qaldProperty ;
                           rdf:type owl:FunctionalProperty ;
                           rdfs:domain <urn:qado#Question> ;
                           rdfs:range xsd:string ;
                           rdfs:comment "Property linking a question to its SPARQL result as stringified JSON object"@en ;
                           rdfs:label "has question SPARQL result"@en .


###  urn:qado#hasSparqlTemplate
<urn:qado#hasSparqlTemplate> rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf :lcquad2Property ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain <urn:qado#Question> ;
                             rdfs:range xsd:string ;
                             rdfs:comment "Question has a SPARQL query template"@en ;
                             rdfs:label "has SPARQL template"@en .


###  urn:qado#hasSparqlTemplateId
<urn:qado#hasSparqlTemplateId> rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf :lcquad1Property ,
                                                  :lcquad2Property ;
                               rdf:type owl:FunctionalProperty ;
                               rdfs:domain <urn:qado#Question> ;
                               rdfs:range xsd:nonNegativeInteger ;
                               rdfs:comment "points to the ID of the used SPARQL template"@en ;
                               rdfs:label "has SPARQL template ID"@en .


###  urn:qado#hasSubgraphType
<urn:qado#hasSubgraphType> rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf :lcquad2Property ;
                           rdf:type owl:FunctionalProperty ;
                           rdfs:domain <urn:qado#Question> ;
                           rdfs:range xsd:string ;
                           rdfs:comment "points to the type of the used subgraph"@en ;
                           rdfs:label "has subgraph type"@en .


###  urn:qado#hasTag
<urn:qado#hasTag> rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :rubqProperty ;
                  rdfs:domain <urn:qado#Question> ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Optional tag of a question."@en ;
                  rdfs:label "has question tag"@en .


###  urn:qado#hasTemplateIndex
<urn:qado#hasTemplateIndex> rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf :lcquad2Property ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain <urn:qado#Question> ;
                            rdfs:range xsd:nonNegativeInteger ;
                            rdfs:comment "Index of the used SPARQL template"@en ;
                            rdfs:label "has SPARQL template index"@en .


###  urn:qado#hasWebqspId
<urn:qado#hasWebqspId> rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :cwqProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain <urn:qado#Question> ;
                       rdfs:range xsd:string ;
                       rdfs:comment "ID of the used web QSP question"@en ;
                       rdfs:label "has Web QSP ID"@en .


###  urn:qado#hasWebqspQuestion
<urn:qado#hasWebqspQuestion> rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf :cwqProperty ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain <urn:qado#Question> ;
                             rdfs:range xsd:string ;
                             rdfs:comment "Question text in web QSP format"@en ;
                             rdfs:label "has Web QSP question text"@en .


###  urn:qado#hasbeenTransferredToAnotherKnowledgeGraph
<urn:qado#hasbeenTransferredToAnotherKnowledgeGraph> rdf:type owl:DatatypeProperty ;
                                                     rdfs:subPropertyOf :basicQADOProperty ;
                                                     rdfs:domain <urn:qado#Question> ;
                                                     rdfs:comment "Property referencing another question instance that is representing the same question text for another Knowledge Graph"@en ;
                                                     rdfs:label "has a question equivalent that was transferred to another Knowledge Graph"@en .


###  urn:qado#isHybrid
<urn:qado#isHybrid> rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :qaldProperty ;
                    rdfs:domain <urn:qado#Question> ;
                    rdfs:range xsd:boolean ;
                    rdfs:comment "Question combines different data sources."@en ;
                    rdfs:label "is hybrid question"@en .


###  urn:qado#isOnlyDBpediaCompatible
<urn:qado#isOnlyDBpediaCompatible> rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf :qaldProperty ;
                                   rdf:type owl:FunctionalProperty ;
                                   rdfs:domain <urn:qado#Question> ;
                                   rdfs:range xsd:boolean ;
                                   rdfs:comment "The given question works only on DBpedia."@en ;
                                   rdfs:label "is only compatible with DBpedia"@en .


###  urn:qado:dataset#basicQADOProperty
:basicQADOProperty rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   owl:propertyDisjointWith :extendedQADOProperty ;
                   rdfs:comment "Property available for all transformed QA benchmarks"@en ;
                   rdfs:label "Basic QADO property"@en .


###  urn:qado:dataset#category
:category rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :mintakaProperty ;
          rdfs:domain <urn:qado#Question> ;
          rdfs:range xsd:string ;
          rdfs:comment "The associated category of a question."@en ;
          rdfs:label "Question category"@en .


###  urn:qado:dataset#complexityType
:complexityType rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :mintakaProperty ;
                rdfs:domain <urn:qado#Question> ;
                rdfs:range xsd:string ;
                rdfs:comment "The associated complexity type of a question."@en ;
                rdfs:label "Complexity type"@en .


###  urn:qado:dataset#cwqProperty
:cwqProperty rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :extendedQADOProperty ;
             rdfs:comment "Property available for all entries of the CWQ benchmark"@en ;
             rdfs:label "CWQ property"@en .


###  urn:qado:dataset#extendedQADOProperty
:extendedQADOProperty rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:comment "Property available only on a subset of QA benchmarks"@en ;
                      rdfs:label "Extended QADO property"@en .


###  urn:qado:dataset#hasAnswerBodyLabel
:hasAnswerBodyLabel rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :mintakaProperty ;
                    rdfs:domain <urn:qado#ValidAnswer> ;
                    rdfs:range xsd:string ;
                    rdfs:comment "The label of an answer body."@en ;
                    rdfs:label "has answer body label"@en .


###  urn:qado:dataset#hasPrefix
:hasPrefix rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :basicQADOProperty ;
           rdfs:domain <urn:qado#Dataset> ;
           rdfs:range xsd:string ;
           rdfs:comment "The URI that has been replaced by a prefix."@en ;
           rdfs:label "has prefix"@en .


###  urn:qado:dataset#lcquad1Property
:lcquad1Property rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :lcquadProperty ;
                 rdfs:comment "Property available for LC-QuAD 1"@en ;
                 rdfs:label "LC-QuAD 1 Property"@en .


###  urn:qado:dataset#lcquad2Property
:lcquad2Property rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :lcquadProperty ;
                 rdfs:comment "Property available for LC-QuAD 2"@en ;
                 rdfs:label "LC-QuAD 2 Property"@en .


###  urn:qado:dataset#lcquadProperty
:lcquadProperty rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :extendedQADOProperty ;
                rdfs:comment "Property available for all entries of the LC-QuAD benchmark"@en ;
                rdfs:label "LC-QuAD property"@en .


###  urn:qado:dataset#mention
:mention rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :mintakaProperty ;
         rdfs:domain <urn:qado#ValidAnswer> ;
         rdfs:range xsd:string ;
         rdfs:comment "A string mentioning the answer of the question."@en ;
         rdfs:label "Answer mention"@en .


###  urn:qado:dataset#mintakaProperty
:mintakaProperty rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :extendedQADOProperty ;
                 rdfs:comment "Property available for all entries of the MINTAKA benchmark"@en ;
                 rdfs:label "MINTAKA property"@en .


###  urn:qado:dataset#normalizedQueryLength
:normalizedQueryLength rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :sparqlQueryProperty ;
                       rdfs:domain <urn:qado#Query> ;
                       rdfs:range xsd:nonNegativeInteger ;
                       rdfs:comment "The length of a SPARQL query with normalized prefixes."@en ;
                       rdfs:label "Normalized SPARQL query length"@en .


###  urn:qado:dataset#numberOfFilters
:numberOfFilters rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :sparqlQueryProperty ;
                 rdfs:domain <urn:qado#Query> ;
                 rdfs:range xsd:nonNegativeInteger ;
                 rdfs:comment "The number of FILTER statements used in a SPARQL query."@en ;
                 rdfs:label "Number of FILTER statements"@en .


###  urn:qado:dataset#numberOfModifierGroupBy
:numberOfModifierGroupBy rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :sparqlQueryProperty ;
                         rdfs:domain <urn:qado#Query> ;
                         rdfs:range xsd:nonNegativeInteger ;
                         rdfs:comment "The number of GROUP BY modifiers of a SPARQL query."@en ;
                         rdfs:label "Number of GROUP BY modifiers"@en .


###  urn:qado:dataset#numberOfModifierHaving
:numberOfModifierHaving rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :sparqlQueryProperty ;
                        rdfs:domain <urn:qado#Query> ;
                        rdfs:range xsd:nonNegativeInteger ;
                        rdfs:comment "The number of HAVING modifiers of a SPARQL query."@en ;
                        rdfs:label "Number of HAVING modifiers"@en .


###  urn:qado:dataset#numberOfModifierLimit
:numberOfModifierLimit rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :sparqlQueryProperty ;
                       rdfs:domain <urn:qado#Query> ;
                       rdfs:range xsd:nonNegativeInteger ;
                       rdfs:comment "The number of LIMIT modifiers of a SPARQL query."@en ;
                       rdfs:label "Number of LIMIT modifiers"@en .


###  urn:qado:dataset#numberOfModifierOffset
:numberOfModifierOffset rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :sparqlQueryProperty ;
                        rdfs:domain <urn:qado#Query> ;
                        rdfs:range xsd:nonNegativeInteger ;
                        rdfs:comment "The number of OFFSET modifiers of a SPARQL query."@en ;
                        rdfs:label "Number of OFFSET modifiers"@en .


###  urn:qado:dataset#numberOfModifierOrderBy
:numberOfModifierOrderBy rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf :sparqlQueryProperty ;
                         rdfs:domain <urn:qado#Query> ;
                         rdfs:range xsd:nonNegativeInteger ;
                         rdfs:comment "The number of ORDER BY modifiers of a SPARQL query."@en ;
                         rdfs:label "Number of ORDER BY modifiers"@en .


###  urn:qado:dataset#numberOfModifiers
:numberOfModifiers rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :sparqlQueryProperty ;
                   rdfs:domain <urn:qado#Query> ;
                   rdfs:range xsd:nonNegativeInteger ;
                   rdfs:comment "The total amount of modifiers used in a SPARQL query."@en ;
                   rdfs:label "Number of modifiers"@en .


###  urn:qado:dataset#numberOfResources
:numberOfResources rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :sparqlQueryProperty ;
                   rdfs:domain <urn:qado#Query> ;
                   rdfs:range xsd:nonNegativeInteger ;
                   rdfs:comment "Number of fixed entities, not variables, used in a SPARQL query"@en ;
                   rdfs:label "Number of resources"@en .


###  urn:qado:dataset#numberOfResourcesPredicates
:numberOfResourcesPredicates rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf :sparqlQueryProperty ;
                             rdfs:domain <urn:qado#Query> ;
                             rdfs:range xsd:nonNegativeInteger ;
                             rdfs:comment "The number of fixed entities, not variables, used as predicates in a SPARQL query."@en ;
                             rdfs:label "Number of resources as predicates"@en .


###  urn:qado:dataset#numberOfResourcesSubjectsObjects
:numberOfResourcesSubjectsObjects rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf :sparqlQueryProperty ;
                                  rdfs:domain <urn:qado#Query> ;
                                  rdfs:range xsd:nonNegativeInteger ;
                                  rdfs:comment "The number of fixed entities, not variables, used in a SPARQL query as subjects or objects."@en ;
                                  rdfs:label "Number of resources as subjects or objects"@en .


###  urn:qado:dataset#numberOfTriples
:numberOfTriples rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :sparqlQueryProperty ;
                 rdfs:domain <urn:qado#Query> ;
                 rdfs:range xsd:nonNegativeInteger ;
                 rdfs:comment "The number of triples used in a WHERE clause of a SPARQL query."@en ;
                 rdfs:label "Number of triples"@en .


###  urn:qado:dataset#numberOfVariables
:numberOfVariables rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :sparqlQueryProperty ;
                   rdfs:domain <urn:qado#Query> ;
                   rdfs:range xsd:nonNegativeInteger ;
                   rdfs:comment "The number of variables defined in a SPARQL query."@en ;
                   rdfs:label "Number of variables"@en .


###  urn:qado:dataset#qaldProperty
:qaldProperty rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :extendedQADOProperty ;
              rdfs:comment "Property available for all entries of the QALD benchmark"@en ;
              rdfs:label "QALD property"@en .


###  urn:qado:dataset#queryType
:queryType rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :sparqlQueryProperty ;
           rdfs:domain <urn:qado#Query> ;
           rdfs:range xsd:string ;
           rdfs:comment "Type of the used SPARQL query (ASK, SELECT, SELECTALL, SELECTDISTINCT, ...)."@en ;
           rdfs:label "SPARQL query type"@en .


###  urn:qado:dataset#questionEntity
