@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qab: <urn:qa:benchmark#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@base <URL#>.


<#DatasetMapping> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "example.json";
        rml:referenceFormulation ql:JSONPath;
        rml:iterator "$.[*]"];
    rr:subjectMap [
        rr:template "urn:qa:benchmark#dataset";
        rr:class qab:Benchmark];
    rr:predicateObjectMap [
        rr:predicate void:dataDump;
        rr:objectMap [
            rr:template "URL"]];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rr:template "LABEL";
            rr:datatype xsd:string]];
    rr:predicateObjectMap [
        rr:predicate foaf:homepage;
        rr:objectMap [
            rr:template "HOMEPAGE";
            rr:datatype xsd:string]].


<#QuestionMapping> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "example.json";
        rml:referenceFormulation ql:JSONPath;
        rml:iterator "$.[*]"];
    rr:subjectMap [
        rr:template "urn:qa:benchmark#question-{uid}";
        rr:class qab:Question];
    rr:predicateObjectMap [
        rr:predicate qab:isElementOf;
        rr:objectMap[
            rr:template "urn:qa:benchmark#dataset"]];
    rr:predicateObjectMap [
        rr:predicate qab:hasId;
        rr:objectMap [
            rml:reference "uid";
            rr:datatype xsd:nonNegativeInteger]];
    rr:predicateObjectMap [
        rr:predicate qab:questionText;
        rr:objectMap [
            rml:reference "question_text";
            rr:language "ru"]];
    rr:predicateObjectMap [
        rr:predicate qab:query;
        rr:objectMap [
            rml:reference "query";
            rr:datatype xsd:string]];
    rr:predicateObjectMap [
        rr:predicate qab:answerText;
        rr:objectMap [
            rml:reference "answer_text";
            rr:language "ru"]];
    rr:predicateObjectMap [
        rr:predicate qab:questionURI;
        rr:objectMap [
            rml:reference "question_uris.[*]";
            rr:termType rr:IRI]];
    rr:predicateObjectMap [
        rr:predicate qab:questionProp;
        rr:objectMap [
            rml:reference "question_props.[*]";
            rr:termType rr:IRI]];
    rr:predicateObjectMap [
        rr:predicate qab:answer;
        rr:objectMap [
            rml:reference "answers.[*].value";
            rr:datatype xsd:string]];
    rr:predicateObjectMap [
        rr:predicate qab:answerType;
        rr:objectMap [
            rml:reference "answers.[*].type";
            rr:datatype xsd:string]];
    rr:predicateObjectMap [
        rr:predicate qab:tag;
        rr:objectMap [
            rml:reference "tags.[*]";
            rr:datatype xsd:string]];
    rr:predicateObjectMap [
        rr:predicate qab:questionEng;
        rr:objectMap [
            rml:reference "question_eng";
            rr:language "en"]].