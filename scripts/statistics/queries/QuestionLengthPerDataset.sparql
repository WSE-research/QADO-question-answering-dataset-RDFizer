PREFIX qab: <urn:qa:benchmark#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?benchmark (MIN(?length) AS ?minLength) (AVG(?length) AS ?avgLength) (MAX(?length) AS ?maxLength)
{
    ?question a ?questionType ;
        qab:isElementOf ?benchmark ;
        qab:hasQuestion ?questionText .
    ?questionType rdfs:subClassOf qab:Question .
    BIND(STRLEN(?questionText) AS ?length)
} GROUP BY ?benchmark ORDER BY ?benchmark